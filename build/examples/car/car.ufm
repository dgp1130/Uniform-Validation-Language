

@filled: /"."/;
@integer: /"^[0-9]{4}$"/;

$("#rootForm") {
	valid: $("#subForm") is valid;
}

$("#subForm") {
    valid: $("#make") matches @filled and
           $("#model") matches @filled and
           $("#year") matches @integer;
}

$("#subForm input") {
    enabled: $("#hasCar") equals true;
}
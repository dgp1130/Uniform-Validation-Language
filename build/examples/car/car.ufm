@filled: /"."/;
@year: /"^[0-9]{4}$"/;

@makeValid: make matches @filled;
make {
    valid: @makeValid;
}

@modelValid: model matches @filled;
model {
    valid: @modelValid;
}

@yearValid: year matches @year;
year {
    valid: @yearValid;
}

valid: @formValid;
@formValid: @makeValid and @modelValid and @yearValid;
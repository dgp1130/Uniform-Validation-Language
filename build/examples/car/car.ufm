@filled: /"."/;
@year: /"^[0-9]{4}$"/;

valid: @carForm.valid;

@carForm {
    valid: @makeValid and @modelValid and @yearValid;

    @makeValid: make matches @filled;
    make {
        valid: @makeValid;
    }

    @modelValid: model matches @filled;
    model {
        valid: @modelValid;
    }

    @yearValid: year matches @year;
    year {
        valid: @yearValid;
    }
}